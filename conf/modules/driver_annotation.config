process{
    withName: 'ANNOTATE_DRIVER' {
        errorStrategy = 'finish'
        publishDir = [
                [
                    mode: params.publish_dir_mode,
                    path: { "${params.outdir}/driver_annotation/annotate_driver/${meta.dataset}/${meta.patient}/${meta.tumour_sample}/"},
                    pattern: "*{rds}"
                ]
            ]
    }
}
